{
    "realtime": {
        "title":"Real-Time",
        "view":"realtime"
    },

    "realtimeVisitors": {
        "title":"realtimeVisitors",
        "uri": "realtime/rt-overview",
        "view":"realtimeVisitors",
        "chart":"counter",
        "enabledCharts":["counter", "table", "pie"],
        "realtime": true,
        "metrics": [
            "rt:activeUsers"
        ],
        "dimensions": [
            "rt:userType"
        ]
    },
